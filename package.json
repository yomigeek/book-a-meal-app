{
  "name": "book",
  "version": "1.0.0",
  "engines": {
    "node": "8.11.1"
  },
  "description": "book a meal app",
  "main": "app.js",
  "scripts": {
    "test": "set NODE_ENV=test&&nyc --reporter=html --reporter=text --reporter=lcov mocha --compilers js:babel-core/register test/testApp.js --exit",
    "build": "rimraf dist/ ",
    "start": "npm run build && node app.js",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "heroku-postbuild": "sequelize db:migrate"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "*",
    "dotenv": "^5.0.1",
    "express": "*",
    "jsonwebtoken": "^8.2.1",
    "morgan": "^1.9.0",
    "node-localstorage": "^1.3.1",
    "pg": "^7.4.1",
    "sequelize": "^4.37.6",
    "sequelize-auto-migrations": "^1.0.2",
    "sequelize-cli": "^4.0.0"
  },
  "author": "Abayomi Olaoye",
  "license": "ISC",
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-http": "^1.0.0",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "istanbul": "^0.4.5",
    "mocha": "^5.1.1",
    "request": "^2.85.0",
    "rimraf": "^2.6.2",
    "should": "^13.2.1"
  },
  "rules": {
    "linebreak-style": 0,
    "global-require": 0,
    "eslint linebreak-style": [
      0,
      "error",
      "windows"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    }
  }
}
