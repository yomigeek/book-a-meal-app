{
  "name": "book",
  "version": "1.0.0",
  "description": "book a meal app",
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/yomigeek/book-a-meal-app.git"
  },
  "scripts": {
    "test": "set NODE_ENV=test&&nyc --reporter=html --reporter=text --reporter=lcov mocha --compilers js:babel-core/register test/testApp.js --exit",
    "build": "rimraf dist/ && babel ./ -- out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files",
    "start": "npm run build && node dist/app.js",
    "coveralls": "npm run cover -- --report lcovonly && cat ./coverage/lcov.info | coveralls",
    "cover": "istanbul cover _mocha"
  },
  "dependencies": {
    "body-parser": "*",
    "express": "*",
    "morgan": "^1.9.0"
  },
  "author": "Abayomi Olaoye",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^3.5.0",
    "chai-http": "^1.0.0",
    "coveralls": "^3.0.0",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "mocha": "^5.1.1",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^11.7.1",
    "request": "^2.85.0",
    "rimraf": "^2.6.2",
    "should": "^13.2.1"
  }
}
